{
  "manifest_version": 2,

  "name": "QuickDesk Addleads Chrome Extension v5",
  "description": "This is my QD extension!",
  "version": "5.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
  	"storage",
    "tabs",
    "https://manage.quickdesk.io",
    "https://quickdesk.io",
    "*://*.quickdesk.io/*",
    "https://manage.quickdesk.io/login"
  ]
  
}